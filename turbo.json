{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [
		".env",
		".env.*",
		"!.env.example",
		"!.env.test",
		"svelte.config.js",
		"vite.config.ts",
		"tsconfig.json"
	],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"],
			"outputs": [".svelte-kit/**", "build/**", "dist/**"]
		},
		"postbuild": {
			"dependsOn": ["build"]
		},
		"lint": {}
	}
}
